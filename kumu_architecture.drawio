        <mxCell id="Nd0iNkwbYfhz8qe4l_91-44" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0)); font-size: 13px;&quot; face=&quot;Helvetica&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;SPAWNS SEPARATE JAVA PROCESS:&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;- R forks child process&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;- Java runs in separate JVM (4-6GB heap)&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;- No shared memory with R&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;- R waits for completion (blocking)&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;- Stdout captured and returned&lt;/span&gt;&lt;/font&gt;" vertex="1">
          <mxGeometry height="140" width="260" x="390" y="1090" as="geometry" />
        </mxCell>
        <mxCell id="Nd0iNkwbYfhz8qe4l_91-45" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;font-size: 10px; color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;Java Process: 4-6GB (SEPARATE)&lt;/span&gt;&lt;br style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;- JVM heap&lt;/span&gt;&lt;br style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;- Tetrad objects&lt;/span&gt;&lt;br style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;- Algorithm state&lt;/span&gt;&lt;/font&gt;&lt;div&gt;&lt;font style=&quot;font-size: 10px; color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;&lt;br style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;br&gt;&lt;/font&gt;&lt;/div&gt;" vertex="1">
          <mxGeometry height="80" width="80" x="1485" y="1130" as="geometry" />
        </mxCell>
        <mxCell id="Nd0iNkwbYfhz8qe4l_91-46" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;&lt;span style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;DISADVANTAGES:&lt;/span&gt;&lt;br style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;- Slow data transfer (file I/O)&lt;/span&gt;&lt;br style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;- Text parsing overhead&lt;/span&gt;&lt;br style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;- No intermediate access&lt;/span&gt;&lt;br style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;- Limited flexibility&lt;/span&gt;&lt;/font&gt;" vertex="1">
          <mxGeometry height="30" width="60" x="2080" y="1130" as="geometry" />
        </mxCell>
        <mxCell id="Nd0iNkwbYfhz8qe4l_91-47" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;font-size: 10px; color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;Communication:&lt;/span&gt;&lt;br style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;R → Java: CLI flags&lt;/span&gt;&lt;br style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;Java → R: stdout text&lt;/span&gt;&lt;br style=&quot;font-weight: 700; text-wrap-mode: nowrap;&quot;&gt;&lt;/font&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;font-weight: 700; text-wrap-mode: nowrap; background-color: transparent;&quot;&gt;&lt;font style=&quot;font-size: 10px; color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;No shared memory!&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;" vertex="1">
          <mxGeometry height="30" width="60" x="1690" y="1130" as="geometry" />
        </mxCell>
        <mxCell id="Nd0iNkwbYfhz8qe4l_91-48" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;font-size: 13px; color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;KEY CHARACTERISTIC: External Process via CLI&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;• R spawns Java using system2() - separate memory spaces&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;• Communication via command-line flags (R to Java) and stdout text (Java to R)&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;• Data loaded from CSV files by Java process&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;• Results parsed from text output by parse_graph()&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;/font&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap; background-color: transparent;&quot;&gt;&lt;font style=&quot;font-size: 13px; color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;• No shared memory - crash-safe but slower&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;" vertex="1">
          <mxGeometry height="30" width="60" x="915" y="2045" as="geometry" />
        </mxCell>
        <mxCell id="Nd0iNkwbYfhz8qe4l_91-49" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0)); font-size: 13px;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;DESIGN PHILOSOPHY: Simple, modular, flag-based composition. Each function returns character vector of flags.&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;Functions compose naturally: tetrad(data_flags, score_flags, algo_flags, boot_flags, knowledge_flags)&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;VS PYKUMU: External process (Kumu) vs in-process JPype (PyKumu). Same algorithms, different architectures.&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;Kumu: Simpler, slower. PyKumu: Complex, faster. Both produce identical results.&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;DEPENDENCIES: R &amp;gt;= 3.6.0, data.table, stringi, Java 8+&lt;/span&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;br style=&quot;text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;text-wrap-mode: nowrap;&quot;&gt;See COMPLETE_ARCHITECTURE.md for detailed documentation.&lt;/span&gt;&lt;/font&gt;" vertex="1">
          <mxGeometry height="70" width="60" x="1840" y="2035" as="geometry" />
        </mxCell>
        <mxCell id="Nd0iNkwbYfhz8qe4l_91-52" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;COMPARISON TO PYKUMU:&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;PyKumu: In-process JPype&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;→ Direct API access&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;→ 1000x faster&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;→ More flexible&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;Kumu: External system2()&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;→ CLI flags only&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;→ Simpler but slower&lt;/span&gt;&lt;/font&gt;" vertex="1">
          <mxGeometry height="30" width="60" x="1750" y="1500" as="geometry" />
        </mxCell>
        <mxCell id="Nd0iNkwbYfhz8qe4l_91-54" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=16;" value="&lt;font style=&quot;color: light-dark(rgb(0, 0, 0), rgb(0, 0, 0));&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;SAME ALGORITHMS:&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;Both Kumu and PyKumu&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;use identical Tetrad library&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;→ Same results&lt;/span&gt;&lt;br style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;→ Different interfaces&lt;/span&gt;&lt;/font&gt;" vertex="1">
